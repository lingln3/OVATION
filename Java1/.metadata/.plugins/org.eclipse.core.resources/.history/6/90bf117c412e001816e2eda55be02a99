package duotaiceshi;
/**
 * 进行Java多态和动态绑定学习
 * @author snpas
 *
 */
public class DuotaiTest {
	Father f = new Son();
}
class Father{
	private int money;
	public void setMoney(int m){
		money = m;
	}
	public int getMoney(){
		return money;
	}
}
class Son extends Father{
	private double money;
	public void setMoney(double m){
		money = m;
	}
	public double getMoney(){
		return money;
	}
}