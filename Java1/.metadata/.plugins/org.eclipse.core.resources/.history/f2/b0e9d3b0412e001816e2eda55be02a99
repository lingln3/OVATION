package duotaiceshi;
/**
 * 进行Java多态和动态绑定学习
 * @author snpas
 *
 */
public class DuotaiTest {
	Father f = new Son();
	f.setMoney(100);
}
class Father{
	private int money;
	public void setMoney(int m){
		money = m;
		System.out.println("调用父类的方法");
	}
	public int getMoney(){
		return money;
	}
}
class Son extends Father{
	private double money;
	public void setMoney(double m){
		money = m;
		System.out.println("调用子类的方法");
	}
	public int getMoney(){
		return (int)money;
	}
}